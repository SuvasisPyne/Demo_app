<h1>Users#index</h1>
<table><tr><th>User Name</th><th>Email</th></tr>
<% @usrs.each do |u| %>
<tr><td><%= u.user_name %></td>
<td><%= u.email %></td>
<td><%= link_to "Show",user_path(u) %></td>
<td><%= link_to "Delete",user_path(u), :method => :delete %></td>
<td><%= link_to "Edit",edit_user_path(u) %></td>
</tr>
<% end %>
</table>
<br><br>
<%= link_to "Create a new user", new_user_path %>
<p>Find me in app/views/users/index.html.erb</p>
